Cell[TextData[{
 "From 2), ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"d\[Lambda]", "(", "t", ")"}], "dt"], "=", " ", "0"}], 
   TraditionalForm]],ExpressionUUID->"0993e2c7-4ded-4049-9e97-803e12ec276e"],
