  FormBox[
   SubscriptBox["\[Lambda]", "0"], TraditionalForm]],ExpressionUUID->
  "89252775-61ad-44c4-8221-0d80f22a1c27"],
 "\nTherefore, from 3), ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["g", "opt"], "(", "t", ")"}], " ", "=", " ", 
     RowBox[{"k", " ", "[", 
      RowBox[{
       RowBox[{"\[Sqrt]", 
        RowBox[{"(", 
         FractionBox[
          SubscriptBox["c", "a"], 
          RowBox[{"\[Alpha]", " ", "D", " ", 
           SubscriptBox["\[Lambda]", "0"]}]], ")"}]}], "-", "1"}], "]"}]}], 
    " ", ","}], TraditionalForm]],ExpressionUUID->
  "f5624b4b-6dbe-42fe-9c1d-137ba55ecb56"],
 " where",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{"\[Alpha]", " ", "=", " ", 
     RowBox[{"\[Nu]", " ", "a", " ", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"n", " ", 
         SubscriptBox["Z", "r"]}], ")"}], 
       RowBox[{"-", "1"}]], "   "}]}]}], TraditionalForm]],ExpressionUUID->
  "88be463a-4038-4a1d-9839-ce007ad40500"],
 ".\nAnd, from 4) x(t) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "0"], " ", "-", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[Beta]", " ", "+", " ", 
       RowBox[{"\[Alpha]", " ", 
        RowBox[{
         SubscriptBox["g", "opt"], "(", "t", ")"}], " ", "D"}]}], ")"}], 
     "t"}]}], TraditionalForm]],ExpressionUUID->
  "88638405-a883-4357-80d9-5ac998192385"],
 ", where \[Beta] = ",
 Cell[BoxData[
